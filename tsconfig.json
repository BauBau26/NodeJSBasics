{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true
  }
}
